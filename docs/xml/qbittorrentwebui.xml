    <module name="qBitTorrentWebUI">
      <short>Component to access the WebUI of qBitTorrent.</short>
      <descr>
        <p>In this unit you can find an implementation of component to access the WebUI of qBitTorrent.</p>
      </descr>
      <!-- unresolved type reference Visibility: default -->
      <element name="Classes">
        <short/>
        <descr/>
        <errors/>
        <seealso/>
      </element>
      <!-- unresolved type reference Visibility: default -->
      <element name="SysUtils">
        <short/>
        <descr/>
        <errors/>
        <seealso/>
      </element>
      <!-- object Visibility: default -->
      <element name="TqBitTorrentWebUI">
        <short>Implements an interface to access the WebUI of qBitTorrent.</short>
        <descr/>
        <errors/>
        <seealso/>
      </element>
      <!-- property Visibility: published -->
      <element name="TqBitTorrentWebUI.IsLogged">
        <short>Contains state of Login</short>
        <descr/>
        <errors/>
        <seealso/>
      </element>
      <!-- property Visibility: published -->
      <element name="TqBitTorrentWebUI.UserName">
        <short>Username to login to the qBittorrent client.</short>
        <descr/>
        <errors/>
        <seealso/>
      </element>
      <!-- property Visibility: published -->
      <element name="TqBitTorrentWebUI.Password">
        <short>Password to login to the qBittorrent client.</short>
        <descr/>
        <errors/>
        <seealso/>
      </element>
      <!-- property Visibility: published -->
      <element name="TqBitTorrentWebUI.Host">
        <short>Host/Address  for the qBittorrent client.</short>
        <descr/>
        <errors/>
        <seealso/>
      </element>
      <!-- property Visibility: published -->
      <element name="TqBitTorrentWebUI.Port">
        <short>Port  for the qBittorrent client.</short>
        <descr/>
        <errors/>
        <seealso/>
      </element>
      <!-- property Visibility: published -->
      <element name="TqBitTorrentWebUI.LoginCookie">
        <short>Cookie value from the Login call.</short>
        <descr/>
        <errors/>
        <seealso/>
      </element>
      <!-- function Visibility: public -->
      <element name="TqBitTorrentWebUI.Login">
        <short>Performs a Login</short>
        <descr/>
        <errors>Will throw an exception if not successfull.</errors>
        <example file="examples/login_example.pas"/>
        <seealso/>
      </element>
      <!-- function result Visibility: default -->
      <element name="TqBitTorrentWebUI.Login.Result">
        <short>True if login succeded.</short>
      </element>
      <!-- function Visibility: public -->
      <element name="TqBitTorrentWebUI.Logout">
        <short>Performs a Logout</short>
        <descr/>
        <errors>Will throw an exception if not successfull.</errors>
        <seealso/>
        <example file="examples/logout_example.pas"/>
      </element>
      <!-- function result Visibility: default -->
      <element name="TqBitTorrentWebUI.Logout.Result">
        <short>True if logout succeded.</short>
      </element>
      <!-- function Visibility: public -->
      <element name="TqBitTorrentWebUI.GetApiVersion">
        <short>Get the WebUI API version.</short>
        <descr/>
        <errors>Will throw an exception if not successfull.</errors>
        <seealso/>
        <example file="examples/getapiversion_example.pas"/>
      </element>
      <!-- function result Visibility: default -->
      <element name="TqBitTorrentWebUI.GetApiVersion.Result">
        <short>A string with the WebUI API version.</short>
      </element>
      <!-- function Visibility: public -->
      <element name="TqBitTorrentWebUI.GetMinApiVersion">
        <short>Get the WebUI minimum accepted API version.</short>
        <descr/>
        <errors>Will throw an exception if not successfull.</errors>
        <example file="examples/getminapiversion_example.pas"/>
        <seealso/>
      </element>
      <!-- function result Visibility: default -->
      <element name="TqBitTorrentWebUI.GetMinApiVersion.Result">
        <short>A string with the WebUI minimum accepted API version.</short>
      </element>
      <!-- function Visibility: public -->
      <element name="TqBitTorrentWebUI.GetqBitTorrentVersion">
        <short>Get qBitTorrent's version.</short>
        <descr/>
        <errors>Will throw an exception if not successfull.</errors>
        <example file="examples/getqbittorrentversion_example.pas"/>
        <seealso/>
      </element>
      <!-- function Visibility: public -->
      <element name="TqBitTorrentWebUI.ExecShutdown">
        <short>Executes the command to shutdown(close) the qBitTorrent client</short>
        <descr/>
        <errors>Will throw an exception if not successfull.</errors>
        <example file="examples/execshutdown_example.pas"/>
        <seealso/>
      </element>
      <!-- function result Visibility: default -->
      <element name="TqBitTorrentWebUI.GetqBitTorrentVersion.Result">
        <short>A string with qBitTorrent's version.</short>
      </element>
      <!-- function result Visibility: default -->
      <element name="TqBitTorrentWebUI.ExecShutdown.Result">
        <short>True if shutdown succeded.</short>
      </element>
    <element name="TqBitTorrentWebUI.Torrents"><short>Contains the list of torrents</short>
      </element>
